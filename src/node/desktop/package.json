{
  "name": "rstudio",
  "productName": "RStudio",
  "version": "9999.99.9-dev+1.test9",
  "description": "RStudio",
  "main": "./.webpack/main",
  "repository": "https://github.com/rstudio/rstudio",
  "author": "RStudio PBC",
  "license": "AGPL-3.0-only",
  "scripts": {
    "clean": "ts-node scripts/clean.ts",
    "lint": "eslint ./src ./test",
    "package": "npm ci && electron-forge package",
    "start": "electron-forge start",
    "debug": "electron-forge start --inspect-electron",
    "fullstart": "npm install && electron-forge start",
    "test": "electron-mocha --no-sandbox --full-trace --config ./test/unit/mocharc.json",
    "testcover": "nyc electron-mocha -c --config ./test/unit/mocharc.json",
    "testwip": "electron-mocha -c -g WIP --config ./test/unit/mocharc.json",
    "format": "npx prettier --write . --ignore-path .gitignore",
    "postinstall": "npx electron-rebuild && ts-node scripts/generate.ts",
    "generate": "ts-node scripts/generate.ts",
    "install-fuses": "node scripts/install-fuses.mjs",
    "automation": "npm start -- -- --run-automation"
  },
  "config": {
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@electron/packager": "18.3.3",
    "@electron-forge/cli": "7.4.0",
    "@electron-forge/plugin-webpack": "7.4.0",
    "@types/chai": "4.3.16",
    "@types/crc": "3.8.3",
    "@types/line-reader": "0.0.37",
    "@types/lodash.debounce": "4.0.9",
    "@types/mocha": "10.0.7",
    "@types/node": "22.0.0",
    "@types/properties-reader": "2.1.3",
    "@types/sinon": "17.0.3",
    "@types/uuid": "10.0.0",
    "@types/winston-syslog": "2.4.3",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "@vercel/webpack-asset-relocator-loader": "1.7.4",
    "chai": "4.4.1",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "7.1.2",
    "electron": "31.3.1",
    "electron-mocha": "13.0.0",
    "eslint": "8.57",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "json": "11.0.0",
    "json-schema-to-typescript": "14.1.0",
    "lint-staged": "15.2.7",
    "mocha": "10.7.0",
    "nan": "2.20.0",
    "node-loader": "2.0.0",
    "nyc": "17.0.0",
    "prettier": "3.3.3",
    "process": "0.11.10",
    "sinon": "17.0.1",
    "style-loader": "4.0.0",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.5.3",
    "vue-loader": "17.4.2",
    "vue-template-compiler": "2.7.16",
    "webpack": "5.93.0"
  },
  "dependencies": {
    "@electron/fuses": "1.8.0",
    "@vueuse/core": "10.11.0",
    "crc": "4.3.2",
    "electron-store": "8.2.0",
    "i18next": "23.12.1",
    "line-reader": "0.4.0",
    "lodash.debounce": "4.0.8",
    "net-ipc": "2.2.0",
    "node-abi": "3.65.0",
    "node-addon-api": "8.1.0",
    "node-system-fonts": "1.0.1",
    "properties-reader": "2.3.0",
    "semver": "7.6.3",
