<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
   xmlns:g="urn:import:com.google.gwt.user.client.ui">
   <ui:style>
     .function {
     display: inline;
   }
   
   .sourceLink {
     cursor: pointer;
   }
   
   .functionBlock {
     display: inline-block;
   }
   
   .frameNumber {
     vertical-align: top;
     width: 1.5em; 
     text-align: right;
     margin-top: 0.3em;
     white-space: nowrap;
   }
   
   .frame {
     clear: both;
   }
   </ui:style>
   <g:Grid styleName="{style.frame}">
      <g:row>
         <g:customCell styleName="{style.frameNumber}">
            <g:Label styleName="ace_constant ace_numeric"
                     ui:field="frameNumber"></g:Label>
         </g:customCell>
         <g:customCell>
            <g:HTMLPanel>
               <g:Label ui:field="functionName" styleName="{style.function}">
               </g:Label>
               <g:Anchor ui:field="sourceLink" 
                       styleName="{style.sourceLink} ace_constant ace_numeric">
               </g:Anchor>
            </g:HTMLPanel>
         </g:customCell>
      </g:row>
   </g:Grid>
</ui:UiBinder> 