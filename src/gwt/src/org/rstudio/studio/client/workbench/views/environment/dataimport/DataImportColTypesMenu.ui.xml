<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    ui:generateFormat='com.google.gwt.i18n.server.PropertyCatalogFactory'
    ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
    ui:generateLocales="default">
    <ui:style type='org.rstudio.studio.client.workbench.views.environment.dataimport.DataImportColTypesMenu.DataImportColTypesMenuCss'>
    .important {
        font-weight: bold;
    }
    .mainPanel {
        background-color: #FFF;
        border: 1px solid #cfd4d8;
        -webkit-box-shadow: 0px 5px 5px 0px rgba(0,0,0,0.25);
        -moz-box-shadow:0px 5px 5px 0px rgba(0,0,0,0.25);
        box-shadow: 0px 5px 5px 0px rgba(0,0,0,0.25);
        font-size: 11px;
        min-width: 70px;
    }
    .sectionPanel {
        border-bottom: 1px solid #cfd4d8;
    }
    .sectionLastPanel {
    }
    .entry {
        cursor: pointer;
        display: block;
        padding-right: 8px;
        padding-left: 8px;
        padding-top: 6px;
        padding-bottom: 6px;
    }
    .entrySelected {
        cursor: pointer;
        display: block;
        padding-right: 8px;
        padding-left: 8px;
        padding-top: 6px;
        padding-bottom: 6px;
        background-color: rgb(234, 246, 255);
    }
    .entry:hover {
        background-color: #bce0fc;
    }
    .entrySelected:hover {
        background-color: #bce0fc;
    }
    .errorMessage {
        padding: 10px;
    }
    </ui:style>
    <g:HTMLPanel styleName="{style.mainPanel}">
        <g:HTMLPanel ui:field="typesPanel_" styleName="{style.sectionPanel}">
            <g:Label text="Guess" styleName="{style.entry}" ui:field="guess_"><ui:attribute name="text" key="guessText"/></g:Label>
            <g:Label text="Character" styleName="{style.entry}" ui:field="character_"><ui:attribute name="text" key="characterText"/></g:Label>
            <g:Label text="Double" styleName="{style.entry}" ui:field="double_"><ui:attribute name="text" key="doubleText"/></g:Label>
            <g:Label text="Integer" styleName="{style.entry}" ui:field="integer_"><ui:attribute name="text" key="integerText"/></g:Label>
            <g:Label text="Numeric" styleName="{style.entry}" ui:field="numeric_"><ui:attribute name="text" key="numericText"/></g:Label>
            <g:Label text="Logical" styleName="{style.entry}" ui:field="logical_"><ui:attribute name="text" key="logicalText"/></g:Label>
            <g:Label text="Date" styleName="{style.entry}" ui:field="date_"><ui:attribute name="text" key="dateText"/></g:Label>
            <g:Label text="Time" styleName="{style.entry}" ui:field="time_"><ui:attribute name="text" key="timeText"/></g:Label>
            <g:Label text="DateTime" styleName="{style.entry}" ui:field="dateTime_"><ui:attribute name="text" key="dateTimeText"/></g:Label>
            <g:Label text="Factor" styleName="{style.entry}" ui:field="factor_"><ui:attribute name="text" key="factorText"/></g:Label>
        </g:HTMLPanel>
        <g:HTMLPanel ui:field="includePanel_">
            <g:Label text="Include" styleName="{style.entry}" ui:field="include_"><ui:attribute name="text" key="includeText"/></g:Label>
            <g:Label text="Skip" styleName="{style.entry}" ui:field="skip_"><ui:attribute name="text" key="skipText"/></g:Label>
            <g:Label text="Only" styleName="{style.entry}" ui:field="only_"><ui:attribute name="text" key="onlyText"/></g:Label>
        </g:HTMLPanel>
        <g:HTMLPanel ui:field="errorPanel_">
            <g:Label styleName="{style.errorMessage}" ui:field="error_"/>
        </g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder> 
